GET     /students                     controllers.ZapisyController.listStudents()
GET     /student/:index               controllers.ZapisyController.retrieveStudent(index: Int)
GET     /lectures                     controllers.ZapisyController.listLectures()
GET     /lecture/:id                  controllers.ZapisyController.retrieveLecture(id: String)

GET     /assets/*file                 controllers.Assets.versioned(path="/public", file: Asset)
